{
	"sourceMaps": false,
	"comments": false,
	"presets": [
		["@babel/preset-env"],
		["@babel/preset-react"]
	],
	"env": {
		"development": {
			"presets": [
				["@babel/preset-env",{
					"modules": false //"umd" (retrocompatibility)
				}],
				["@babel/preset-react", {
					"pragma": "e",
					"pragmaFrag": "f",
					"throwIfNamespace": false,
					"runtime": "classic"
				}]
			]
		}
	}
}